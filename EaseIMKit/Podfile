# Uncomment the next line to define a global platform for your project
use_frameworks!

platform :ios, '11.0'

workspace 'EaseIMKit.xcworkspace'

target 'EaseIMWindowTest' do
  platform :ios, '11.0'
  project '../../EaseIMWindowTest/EaseIMWindowTest.xcodeproj'
  pod 'Masonry'
  pod 'Hyphenate'
  pod 'EaseIMKit', :path => './'
end

target 'EaseIM' do
  platform :ios, '11.0'
  project '../../EaseIM/EaseIM/EaseIM.xcodeproj'
  pod 'Masonry'
  pod 'Hyphenate'
  pod 'MJRefresh'
  #pod 'EaseIMKit'
  pod 'EaseIMKit', :path => './'
end

target 'EaseIMKitDemo' do
  platform :ios, '11.0'
  project '../EaseIMKitDemo/EaseIMKitDemo.xcodeproj'
  pod 'Masonry'
  pod 'Hyphenate'
  #pod 'EaseIMKit', :path => './'
end


target 'EaseIMKit' do
  # Pods for EaseIMKit
  pod 'Hyphenate'
  pod 'Masonry'
  pod 'SDWebImage'
  pod 'MBProgressHUD'
  pod 'FLAnimatedImage'
  pod 'EMVoiceConvert'

end
